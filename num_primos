#include<stdio.h>

void primos (int n);

int main(){

int n;


printf("Introduzca el numero:\n");
scanf("%d", &n);
primos(n);

}

void primos(int n){

int i;
int c=0;

	for(i=1;i<=n;i++){
		if(n%i==0)c++;
	}

	if (c==2) {
		printf("El numero es primo\n");
	}
	else{
		printf("El numero no es primo\n");
	}
} 
